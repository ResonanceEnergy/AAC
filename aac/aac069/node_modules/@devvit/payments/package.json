{
  "name": "@devvit/payments",
  "version": "0.12.12",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://developers.reddit.com/"
  },
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./client": {
      "browser": "./client/index.js",
      "default": "./client/clientImportInServerCodePanic.js"
    },
    "./server": {
      "browser": "./server/serverImportInClientCodePanic.js",
      "default": "./server/index.js"
    },
    "./shared": "./shared/index.js",
    "./helpers/*": {
      "type": "./helpers/*.d.ts",
      "import": "./helpers/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "files": [
    "**"
  ],
  "scripts": {
    "build": "tsc && yarn build:min",
    "build:min": "esbuild --bundle --sourcemap=linked --target=es2020 --format=esm --metafile=dist/meta.min.json --outfile=dist/payments.min.js --external:@devvit/protos --minify src/index.ts",
    "clean": "rm -rf .turbo coverage dist",
    "clobber": "yarn clean && rm -rf node_modules",
    "lint": "redlint .",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "publish-package-json",
    "test": "yarn test:unit && yarn test:types && yarn lint",
    "test:size": "filesize",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit-with-coverage": "vitest run --coverage"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@devvit/protos": "0.12.12",
    "@devvit/public-api": "0.12.12",
    "@devvit/server": "0.12.12",
    "@devvit/shared-types": "0.12.12"
  },
  "devDependencies": {
    "@ampproject/filesize": "4.3.0",
    "@devvit/repo-tools": "0.12.12",
    "@devvit/tsconfig": "0.12.12",
    "esbuild": "0.25.9",
    "eslint": "9.11.1",
    "typescript": "5.8.3",
    "vitest": "4.0.15"
  },
  "filesize": {
    "dist/payments.min.js": {
      "none": "290 KB",
      "gzip": "80 KB"
    }
  },
  "source": "./src/index.ts",
  "gitHead": "045f2f9f7f9871b9803dcee7e26cb187ef85b923"
}
